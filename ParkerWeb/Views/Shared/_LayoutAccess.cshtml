<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Casa Parker Project</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/Site.css")
</head>
<body>



    <section class="h-100 gradient-form" style="background-color: #FFE2C8;">
        <div class="container py-5 h-100">
            <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col-xl-11">
                    <div class="card rounded-3 text-black">
                        <div class="row g-0">
                            <div class="col-lg-7">
                                <div class="card-body p-md-5 mx-md-4">
                                    @RenderBody()
                                </div>
                            </div>
                            <div class="col-lg-5 d-flex align-items-center gradient-custom-2">
                                <div class="text-black px-3 py-4 p-md-5 mx-md-4">
                                    <div class="text-center">
                                        <img src="https://drive.google.com/uc?export=view&id=1DOnRbetEMPOc2C-GI49c7Xh4mxLUfz2Y"
                                             style="width: 250px;" alt="logo">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>



    @*<div id="AccessFund">
            @RenderBody()
        </div>*@
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/complementos")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>





